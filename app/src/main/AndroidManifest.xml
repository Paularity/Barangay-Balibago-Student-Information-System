<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.christian.barangaybalibagostudentinformationsystem">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentList"
            android:label="Student List"
            android:largeHeap="true"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".RegisterStudent"
            android:label="Register Student"
            android:largeHeap="true"
            android:parentActivityName=".NavigationActivity" />
        <activity
            android:name=".NavigationActivity"
            android:largeHeap="true"
            android:parentActivityName=".MainActivity" />
        <activity
        android:name=".DailyRecordList"
        android:largeHeap="true"
        android:parentActivityName=".MainActivity" />
        <activity
        android:name=".ResidentNavigationActivity"
        android:largeHeap="true"
        android:parentActivityName=".MainActivity" />

        <activity
            android:name=".ResidentViewProfile"
            android:largeHeap="true"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ResidentViewID"
            android:largeHeap="true"
            android:parentActivityName=".MainActivity" />

    </application>

</manifest>